{
  "name": "chatv2",
  "version": "1.0.0",
  "description": "chat with using react_redux_webSocket_nodeJS",
  "main": "index.js",
  "author": "wolf",
  "license": "MIT",
  "scripts": {
    "conf": "cd client && yarn && cd .. && cd server && yarn && cd .. ",
    "start:client": "cd client && yarn run start",
    "start:server": "cd server && nodemon app.js",
    "start:db": "cd server && mongod --dbpath data/db --port 3001",
    "s": "concurrently  \" yarn run start:db  \"  \" yarn run start:server  \" \" yarn run start:client \""
  },
  "dependencies": {
    "@material-ui/core": "^1.4.0",
    "concurrently": "^3.5.1"
  },
  "devDependencies": {
    "add": "^2.0.6",
    "webpack-dev-server": "^3.1.4"
  }
}
